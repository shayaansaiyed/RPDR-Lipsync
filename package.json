{
  "homepage": "http://shayaansaiyed.github.io/RPDR-Lipsync",
  "name": "rpdr_lipsync_game",
  "version": "1.0.0",
  "description": "A game to pick the winner of a lip sync from all seasons of RPDR",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass --output-style compressed -o dist/css src/scss",
    "serve": "browser-sync start --server --files 'dist/css/*.css, **/*.html'",
    "watch:css": "onchange 'src/scss' -- npm run scss",
    "start": "run-p serve watch:css"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "^2.26.14",
    "node-sass": "^5.0.0",
    "onchange": "^7.1.0"
  },
  "dependencies": {}
}
